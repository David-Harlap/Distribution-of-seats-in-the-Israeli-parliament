   "source": [
    "define function that recive lambda function, number that represents the number of seats to be divided and the name of the column which we will store the information. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 165,
   "metadata": {},
   "outputs": [],
   "source": [
    "def divisor_method(funct, seat_to_give,name):\n",
    "    df[name]=0\n",
    "    for i in range(seat_to_give):\n",
    "        df['temp'] = df['people'] / funct(df[name])\n",
    "        df[name][df['temp'].idxmax()]+=1\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 166,
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
