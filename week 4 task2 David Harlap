   "cell_type": "code",
   "execution_count": 170,
   "metadata": {},
   "outputs": [],
   "source": [
    "def divisor_method_with_threshold(funct, seat_to_give,name):\n",
    "    df1[name]=0\n",
    "    for i in range(seat_to_give):\n",
    "        df1['temp'] = df1['people'] / funct(df1[name])\n",
    "        df1[name][df1['temp'].idxmax()]+=1\n"
   ]
